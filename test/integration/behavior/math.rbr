assert_eq(1 + 2, 3)
assert_eq(2 * 3, 6)
assert_eq(3 - 2, 1)
assert_eq(5 / 2, 2)
assert_eq(5 % 2, 1)

assert(1 == 1)
assert(!(1 == 2))

assert(!(1 != 1))
assert(1 != 2)

assert(1 >= 1)
assert(!(1 >= 2))

assert(1 <= 1)
